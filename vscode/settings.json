{
  "cSpell.userWords": [
    "Cheatsheet",
    "checkmark",
    "cmyk",
    "createdb",
    "else's",
    "Falsey",
    "Figma",
    "Flowcode",
    "Flowcodes",
    "Flowpage",
    "Flowpages",
    "Flowtag",
    "Ghibli",
    "hubspot",
    "Klaviyo",
    "mailchimp",
    "MMDD",
    "optimizely",
    "Palooza",
    "Pangram",
    "postgres",
    "Postgres",
    "Prefs",
    "Prismic",
    "psql",
